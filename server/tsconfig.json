{
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "build",
    "target": "es5",
    "module": "CommonJS",
    "moduleResolution": "node",
    "strict": true,
    "downlevelIteration": true,
    "paths": {
      "*": [
        "../common/*",
        "./server-model/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts"
  ]
}
