{
  "name": "ccsc",
  "version": "0.1.0",
  "private": true,
  "description": "Cor de Cambra Sant Cugat",
  "engines": {
    "node": "12.x"
  },
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "build": "cd client && npm install && npm run build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "author": "Albert Ma√±osa",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/albertms10/ccsc.git"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
